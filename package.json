{
    "name": "poe-filter-language",
    "displayName": "poe-filter-language",
    "description": "A Path of Exile Filter langauge extension by NeverSink",
    "publisher": "NeverSink",
    "version": "1.0.0",
    "repository": {
        "url": "https://github.com/NeverSinkDev/VS-Code-PoE-Filter-Markup-Extension",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "filter",
            "aliases": ["filter", "filter"],
            "extensions": [".filter"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "filter",
            "scopeName": "text.html",
            "path": "./syntaxes/filter.tmLanguage.json"
        }]
    }
}