{
    "name": "poe-filter-language",
    "displayName": "poe-filter-language",
    "description": "A Path of Exile Filter langauge extension by NeverSink",
    "publisher": "NeverSink",
    "version": "2.0.1",
    "repository": {
        "url": "https://github.com/NeverSinkDev/VS-Code-PoE-Filter-Markup-Extension",
        "type": "git"
    },
    "engines": {
        "vscode": "^1.3.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "filter",
                "aliases": [
                    "filter",
                    "filter"
                ],
                "extensions": [
                    ".filter"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "filter",
                "scopeName": "text.html",
                "path": "./syntaxes/filter.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "generator-code": "^1.4.12",
        "yo": "^4.0.0"
    },
    "devDependencies": {
        "webpack": "^5.36.0",
        "webpack-cli": "^4.6.0"
    }
}
